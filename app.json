{
  "expo": {
    "name": "Расписание",
    "slug": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./components/assets/icon.png",
    "splash": {
      "image": "./components/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.patrol.max"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./components/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.max_patrol.myapp"
    },
    "web": {
      "favicon": "./components/assets/icon.png"
    }
  }
}
